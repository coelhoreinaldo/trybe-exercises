const person = {
name: 'Alberto',
lastName: 'Gomes',
age: 20,
};

const info = {
age: 23,
job: 'engenheiro',
};

const family = {
children: ['Maria', 'João'],
wife: 'Ana',
};

Object.assign(person, info, family)
console.log(person)

/* Output
{ name: 'Alberto',
lastName: 'Gomes',
age: 23,
job: 'engenheiro',
children: [ 'Maria', 'João' ],
wife: 'Ana'
} */

